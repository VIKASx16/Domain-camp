#include <iostream>
using namespace std;

int add(int a, int b) {
    return a + b;
}

float add(float a, float b) {
    return a + b;
}

int add(int a, int b, int c) {
    return a + b + c;
}

int main() {
    cout << "Adding two integers (6 + 10): " << add(6, 10) << endl;
    cout << "Adding two floats (5.5 + 10.5): " << add(5.5f, 10.5f) << endl;
    cout << "Adding three integers (11 + 2 + 3): " << add(11, 2, 3) << endl;

    return 0;
}
